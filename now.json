{
    "alias": "grxy",
    "builds": [
        {
            "src": "projects/apollo-server/src/index.js",
            "use": "@now/node-server"
        },
        { "src": "projects/next/package.json", "use": "@now/next" }
    ],
    "github": {
        "autoJobCancelation": true
    },
    "routes": [
        { "src": "/_graphql", "dest": "/projects/apollo-server/src/index.js" },
        { "src": "/(.*)", "dest": "/projects/next/$1" }
    ],
    "version": 2
}
